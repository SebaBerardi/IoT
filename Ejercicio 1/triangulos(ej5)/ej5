#include <stdio.h>  
#include <stdint.h> 
#include <math.h> 


int a = 25;
int b = 25;
int c = 25;
char tipo = 'C';

int main() {
    if (a == b && a==c && b==c) {
        tipo = "A"
    } else {
    tipo = "C"
    }
    if (a == b) {
        tipo = "B"
    } else {
    }
    if (b == c) {
        tipo = "B"
    } else {
    }
    if (a == c) {
        tipo = "B"
    } else {
    }



    switch (tipo) {
        case 'A': printf("Equilatero\n"); break;
        case 'B': printf("Isosceles\n"); break;
        default: printf("Escaleno\n");
    }
    
}
